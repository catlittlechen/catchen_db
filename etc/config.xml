<config>
	<server>
		<bindaddr>0.0.0.0:13570</bindaddr>
		<path>/ccdb</path>
	</server>

	<log>../etc/log.xml</log>

	<data>
		<datapath>../data/</datapath>
		<dataname>ccdb.zjm</dataname>
		<datatimes>60</datatimes>
	</data>

	<masterslave>
		<ismaster>false</ismaster>
		<hashsize>10</hashsize>
		<reflushs>60</reflushs>
	</masterslave>

	<pagesize>256</pagesize>
	<maxonlyuserconnection>10</maxonlyuserconnection>
	<maxuserconnection>10</maxuserconnection>
</config>
